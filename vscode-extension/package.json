    "configuration": {
      "title": "Project Master",
      "properties": {
        "projectMaster.apiUrl": {
          "type": "string",
          "default": "http://localhost:8080",
          "description": "Project Master API URL"
        },
        "projectMaster.apiKey": {
          "type": "string",
          "default": "test-api-key-12345",
          "description": "Project Master API Key"
        },
        "projectMaster.webUrl": {
          "type": "string",
          "default": "http://localhost:3000",
          "description": "Project Master Web UI URL"
        },
        "projectMaster.autoRefresh": {
          "type": "boolean",
          "default": true,
          "description": "Automatically refresh data every 30 seconds"
        },
        "projectMaster.refreshInterval": {
          "type": "number",
          "default": 30000,
          "description": "Auto refresh interval in milliseconds"
        },
        "projectMaster.defaultProject": {
          "type": "string",
          "default": "",
          "description": "Default project ID for new tasks"
        },
        "projectMaster.enableNotifications": {
          "type": "boolean",
          "default": true,
          "description": "Enable desktop notifications"
        },
        "projectMaster.logLevel": {
          "type": "string",
          "enum": [
            "debug",
            "info",
            "warn",
            "error"
          ],
          "default": "info",
          "description": "Logging level"
        },
        "projectMaster.cache.enabled": {
          "type": "boolean",
          "default": true,
          "description": "Enable caching for improved performance"
        },
        "projectMaster.cache.maxSize": {
          "type": "number",
          "default": 1000,
          "minimum": 100,
          "maximum": 10000,
          "description": "Maximum number of cache entries"
        },
        "projectMaster.cache.defaultTtl": {
          "type": "number",
          "default": 300000,
          "minimum": 60000,
          "maximum": 3600000,
          "description": "Default cache TTL in milliseconds (5 minutes)"
        },
        "projectMaster.cache.preloadOnStartup": {
          "type": "boolean",
          "default": true,
          "description": "Preload frequently used data on extension startup"
        },
        "projectMaster.cache.autoCleanup": {
          "type": "boolean",
          "default": true,
          "description": "Automatically clean up expired cache entries"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "test:unit": "mocha out/test/suite/**/*.test.js",
    "package": "vsce package --allow-star-activation",
    "publish": "vsce publish"
  },
