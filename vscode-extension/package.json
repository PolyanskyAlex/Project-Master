      "activitybar": [
        {
          "id": "projectMaster",
          "title": "Project Master",
          "icon": "$(project)"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "projectMaster.refreshProjects",
          "when": "view == projectMaster.projects",
          "group": "navigation"
        },
        {
          "command": "projectMaster.createProject",
